    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "eslint": "^3.9.1",
    "eslint-config-canonical": "^4.0.0",
    "husky": "^0.11.9",
    "webpack": "^2.1.0-beta.4",
    "webpack-dev-server": "^2.0.0-beta"
  },
  "dependencies": {
    "chalk": "^1.1.1",
