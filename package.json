  "license": "BSD-3-Clause",
  "version": "3.4.1",
  "description": "Forces webpack-dev-server to write bundle files to the file system.",
  "main": "./dist/index.js",
  "author": {
    "name": "Gajus Kuizinas",
    "email": "gk@anuary.com",
    "url": "http://gajus.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/write-file-webpack-plugin"
  },
  "keywords": [
    "webpack",
    "plugin",
    "webpack-dev-server",
